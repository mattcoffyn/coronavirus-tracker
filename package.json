{
  "name": "print-factory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@keystonejs/apollo-helpers": "^5.1.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "apollo-upload-client": "^14.1.3",
    "auth0-js": "^9.13.2",
    "babel-plugin-styled-components": "^1.10.7",
    "bulma": "^0.8.2",
    "bulma-switch": "^2.0.0",
    "downshift": "^5.2.5",
    "framer-motion": "^3.2.1",
    "graphql": "^15.0.0",
    "graphql-upload": "^11.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jwt-decode": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.1",
    "next": "^10.0.4",
    "next-google-fonts": "^1.2.1",
    "next-with-apollo": "^5.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-apollo": "^3.1.5",
    "react-datepicker": "^3.4.1",
    "react-dom": "16.13.1",
    "react-icons": "^3.11.0",
    "react-spring": "^8.0.27",
    "styled-components": "^5.1.0",
    "uuid": "^8.3.2"
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "dotenv-webpack": "^1.7.0",
    "eslint": "^7.20.0",
    "eslint-plugin-react": "^7.22.0"
  }
}
